{
  "plugins": [
    "unicorn"
  ],
  "extends": "plugin:unicorn/recommended",
  "rules": {
    "unicorn/no-null": "off",
    "unicorn/prefer-top-level-await": "off",
    "unicorn/no-array-for-each": "off",
    "unicorn/prevent-abbreviations": [
      "error",
      {
        "allowList": {
          "e2e": true,
          "param": true,
          "Param": true
        }
      }
    ]
  }
}
